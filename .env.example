CRON_BIRTHDAY=0 8 * * *
# Default: Monday at 8:00 AM
CRON_PLAN_EXPIRATION=0 8 * * 1
# Default: 8:00 AM every day
CRON_INVOICES=0 8 * * *
# Default: Every 5 minutes
CRON_CHECKIN=*/5 * * * *

# Plan Expiration Settings
PLAN_EXPIRATION_DAYS=14

# Check-in Settings
CHECKIN_ALLOWED_TYPES=Pilates 1x na Semana,Pilates 2x na Semana,Pilates 3x na Semana
CHECKIN_ALLOWED_STATUSES=1
